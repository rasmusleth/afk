<!-- What we put in here (at least for index-requests(i.e. no resources)) will be imported into the body-tag in our layout-file -->
<h2 class="page-header">Recently Added</h2>
<div>
  <% books.forEach(book => { %>
    <a href="/books/<%= book.id %> ">
      <img height="150" width="100" src="<%= book.coverImagePath %>" alt="">
    </a>
  <% }) %>
</div>